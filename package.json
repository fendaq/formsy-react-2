{
  "name": "formsy-react-2",
  "version": "0.20.2",
  "description": "A form input builder and validator for React JS",
  "repository": {
    "type": "git",
    "url": "https://github.com/st-andrew/formsy-react-2.git"
  },
  "main": "lib/main.js",
  "browser": "browser.js",
  "scripts": {
    "prepublishOnly": "npm run test && npm run build && npm run browser",
    "build": "babel ./src/ -d ./lib/",
    "browser": "browserify ./src/main.js -o browser.js -t [ babelify --presets [ env react ] --plugins [ transform-class-properties ] ]",
    "test": "babel-node testrunner"
  },
  "author": "Andrew MacCuaig",
  "license": "MIT",
  "keywords": [
    "react",
    "form",
    "forms",
    "validation",
    "react-component"
  ],
  "dependencies": {
    "form-data-to-object": "^0.2.0",
    "prop-types": "^15.5.7"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "eslint": "^4.0.0",
    "eslint-plugin-react": "^7.0.1",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "nodeunit": "^0.11.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "sinon": "^2.3.4"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.5.0"
  }
}
